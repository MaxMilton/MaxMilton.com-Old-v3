{
  "name": "maxmiltoncom",
  "version": "2.0.0-unreleased",
  "description": "The personal website of Max Milton",
  "repository": "https://github.com/MaxMilton/MaxMilton.com",
  "author": "Max Milton <max@wearegenki.com>",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "help": "lasso --help",
    "helpm": "marko --help",
    "dev-clean": "rm -rf ./.cache/* ./out/*",
    "dev-copy": "cp src/index.html out/index.html",
    "dev-lasso-dev": "lasso src/main.css --main src/main.js --plugins lasso-marko --plugins lasso-postcss --inject-into out/index.html --output-dir out/static/ --development",
    "dev-lasso": "NODE_ENV=production lasso src/main.css --main src/main.js --plugins lasso-marko --plugins lasso-postcss --inject-into out/index.html --output-dir out/static/ --production",
    "dev-minify": "###(cd out/static && find . -type f -name '*.js' -exec uglifyjs --compress --mangle -o \"{}\" -- \"{}\" \\;)",
    "dev-gzip": "(cd out/static && find . -type f ! -name '*.gz' -exec gzip --best \"{}\" \\;)",
    "dev-serve": "NODE_ENV=production http-server ./out/ --gzip --cors",
    "dev-build-dev": "npm run dev-copy && npm run dev-lasso-dev && npm run dev-serve",
    "dev-build": "npm run dev-clean && npm run dev-copy && npm run dev-lasso && npm run dev-minify && npm run dev-gzip && npm run dev-serve",
    "serve": "NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@lasso/marko-taglib": "^1.0.8",
    "@wearegenki/ui": "^0.1.0",
    "lasso": "^3.1.0",
    "lasso-marko": "^2.4.0",
    "marko": "^4.7.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "css-mqpacker": "^6.0.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "http-server": "^0.10.0",
    "lasso-cli": "^2.0.11",
    "lasso-postcss": "^0.3.0",
    "postcss": "^6.0.14",
    "postcss-at-rules-variables": "^0.1.2",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-conditionals": "^2.1.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-processor-html": "^1.0.0",
    "uglify-es": "^3.2.2"
  },
  "engines": {
    "node": ">= 9.0.0",
    "npm": ">= 3.10.0"
  },
  "browserslist": {
    "production": [
      "> 1% in AU",
      "> 1% in JP",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 ChromeAndroid versions",
      "last 2 Edge versions",
      "last 2 iOS versions",
      "last 2 Safari versions",
      "not Explorer <= 11",
      "not ExplorerMobile <= 11"
    ],
    "development": [
      "last 1 Chrome version"
    ]
  }
}
