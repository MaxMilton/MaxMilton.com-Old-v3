{
  "name": "maxmiltoncom",
  "version": "2.0.0-alpha.0",
  "private": true,
  "description": "The personal website of Max Milton",
  "repository": "https://github.com/MaxMilton/MaxMilton.com",
  "author": "Max Milton <max@wearegenki.com>",
  "scripts": {
    "start": "npm run dev",
    "clean": "rm -rf dist && mkdir dist",
    "static": "cp static/* dist/",
    "dev": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=development rollup -c --watch",
    "prebuild": "npm run clean && npm run static",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production rollup -c",
    "serve": "serve -n -l -s -p 1234 dist",
    "_make": "OVERRIDE_ENV=development npm run build && npm run serve",
    "make": "OVERRIDE_ENV=development npm run build && testserver dist",
    "__make": "npm run build && npm run serve"
  },
  "dependencies": {
    "@hyperapp/router": "0.4.1",
    "@wearegenki/ui": "0.5.1",
    "hyperapp": "1.1.2"
  },
  "devDependencies": {
    "browser-sync": "2.23.6",
    "connect-history-api-fallback": "1.5.0",
    "eslint": "4.18.2",
    "postcss": "6.0.19",
    "rollup": "0.56.5",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-node-resolve": "3.2.0",
    "rollup-plugin-postcss": "1.3.3",
    "rollup-plugin-uglify": "3.0.0",
    "stylelint": "9.1.1"
  },
  "engines": {
    "node": ">= 9.6.0"
  }
}
