{
  "name": "maxmiltoncom",
  "version": "2.0.0-unreleased",
  "description": "The personal website of Max Milton",
  "repository": "https://github.com/MaxMilton/MaxMilton.com",
  "author": "Max Milton <max@wearegenki.com>",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV='development' rollup -c -w",
    "test": "npm run lint && npm run unit",
    "pretest": "npm run build",
    "build": "rm -rf lib/* && npm run build:css && npm run build:js",
    "build:css": "postcss -o lib/index.css src/css/main.css",
    "build:js": "NODE_ENV='production' rollup -c",
    "lint": "npm run lint:css && npm run lint:js",
    "lint:css": "stylelint \"src/**/*.marko\" \"src/**/*.css\"",
    "lint:js": "eslint --ext .js,.marko src test",
    "unit": "jest --config test/unit/jest.conf.js --coverage"
  },
  "peerDependencies": {
    "codemirror": "^5.33.0",
    "marko": "^4.7.4"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "css-mqpacker": "^6.0.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-compat": "^2.1.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "postcss": "^6.0.14",
    "postcss-at-rules-variables": "^0.1.2",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-conditionals": "^2.1.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-each": "^0.10.0",
    "postcss-import": "^11.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "stylelint-processor-html": "^1.0.0"
  },
  "engines": {
    "node": ">= 9.0.0",
    "npm": ">= 3.10.0"
  },
  "browserslist": {
    "production": [
      "> 1% in AU",
      "> 1% in JP",
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 ChromeAndroid versions",
      "last 2 Edge versions",
      "last 2 iOS versions",
      "last 2 Safari versions",
      "not Explorer <= 11",
      "not ExplorerMobile <= 11"
    ],
    "development": [
      "last 1 Chrome version"
    ]
  }
}
